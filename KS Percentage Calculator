a = float(input("English Marks = "))
b = float(input("Hindi Marks   = "))
c = float(input("Maths Marks   = "))
d = float(input("Science Marks = "))
e = float(input("SST Marks     = "))
f = float(input("AI Marks      = "))
j = float(input("Computer Marks = "))
print("")
g = float(input("Marks are out of = "))
h = float(input ("total no.of subjects = "))
i = g*h
sum = (a+b+c+d+e+f+j)/i*100
print("")
Name = (input("What is your name? "))
print(" ")
print(Name, "got", sum)
if sum < 33 :
    print("Oh!", Name, "is fail")
if sum >= 33 :
    print(Name, "is pass")
if sum >= 90 :
    print("Hurray!",Name, "is in class toppers list")
