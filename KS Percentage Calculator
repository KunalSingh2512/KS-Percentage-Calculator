a = int(input("English Marks = "))
b = int(input("Hindi Marks   = "))
c = int(input("Maths Marks   = "))
d = int(input("Science Marks = "))
e = int(input("SST Marks     = "))
f = int(input("AI Marks      = "))
j = int(input("Computer Marks = "))
print("")
g = float(input("Marks are out of = "))
h = float(input ("total no.of subjects = "))
i = g*h
sum = (a+b+c+d+e+f+j)/i*100
print("")
Name = str(input("What is your name? "))
print(" ")
print(Name, "got", sum)
if sum <= 33 :
    print("Oh!", Name, "is fail")
if sum >= 33 :
    print(Name, "is pass")
if sum >= 90 :
    print("Hurray!",Name, "is in class toppers list")
